<view class="index">
	<view class="kind">
		<block wx:for="{{dates}}" wx:for-item="i">
			<view class="date">
				<text>
					{{i}}
				</text>
			</view>
			<view class="kind-lists">
				<block wx:for="{{list[i]}}" wx:for-item="list">
					<view class="kind-list">
						<image src="http://wx.qlogo.cn/mmopen/vi_32/Q0j4TwGTfTKiauVH65XNEZ9V7BmKxOXgPibfd5XMn6V1CCiblVMfVa4iacyibAudeicAq9HFcTZG9R2wK1NtNegKZrdw/0" class="image"></image>
						<text class="text-left">
							{{list.usety}}
						</text>
						<text class="text-right">
							{{list.money}}
						</text>
					</view>
				</block>
			</view>
		</block>
	</view>
</view>