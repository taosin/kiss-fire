/**index.wxss**/
/*@import "../common/index.wxss";*/
.index{
	background: #fbf9fe;
	height: 100%;
	padding: 10rpx 0;
}

.kind-lists{
	background: #fff;
	padding: 10rpx 0 10rpx 100rpx;
}
.kind-list{
	font-size: 10pt;
	border-bottom: 1rpx solid #ddd;
	padding: 10rpx 30rpx 0 10rpx;
	clear: both;
	height: 80rpx;
}
.kind-list:last-child{
	border: none;
}
.date{
	font-size: 10pt;
	color: #888;
	padding: 10rpx 40rpx;
}
.text-left{
	float: left;
}
.text-right{
	float: right;
}
.image{
	width: 60rpx;
	height: 60rpx;
	border-radius: 60rpx;
	float: left;
	margin-left: -80rpx;
	margin-top: 5rpx;
}